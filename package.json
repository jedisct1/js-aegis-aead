{
  "name": "aegis-aead",
  "version": "0.2.1",
  "description": "AEGIS authenticated encryption algorithms",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:example": "bun build ./examples/entry.ts --outfile=examples/aegis.bundle.js --target=browser --minify",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/ tests/",
    "format": "biome check --write src/ tests/",
    "bench": "bun run bench/benchmark.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/bun": "^1.3.5",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "aegis",
    "aead",
    "encryption",
    "cryptography",
    "authenticated-encryption",
    "aes",
    "cipher",
    "mac"
  ],
  "author": "Frank Denis <github at pureftpd.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jedisct1/js-aegis-aead.git"
  },
  "homepage": "https://github.com/jedisct1/js-aegis-aead#readme",
  "bugs": {
    "url": "https://github.com/jedisct1/js-aegis-aead/issues"
  },
  "engines": {
    "node": ">=18"
  }
}